<p id="notice"><%= notice %></p>

<h1>Weight History2s</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Weight</th>
      <th>Memo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weight_history2s.each do |weight_history2| %>
      <tr>
        <td><%= weight_history2.user_id %></td>
        <td><%= weight_history2.weight %></td>
        <td><%= weight_history2.memo %></td>
        <td><%= link_to 'Show', weight_history2 %></td>
        <td><%= link_to 'Edit', edit_weight_history2_path(weight_history2) %></td>
        <td><%= link_to 'Destroy', weight_history2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Weight History2', new_weight_history2_path %>
